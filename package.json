{
  "name": "decordator",
  "version": "0.0.1-devel.0",
  "description": "Decordator: discord.js framework using typescript's experimental decorators.",
  "main": "build/mod.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsRauf/decordator.git"
  },
  "keywords": [
    "discord.js",
    "discord",
    "decorators",
    "typescript"
  ],
  "author": "Rauf",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ItsRauf/decordator/issues"
  },
  "homepage": "https://github.com/ItsRauf/decordator#readme",
  "scripts": {
    "precompile": "pnpm run lint",
    "compile": "swc lib -d build",
    "postcompile": "node scripts/d.ts",
    "lint": "eslint lib/**/*.ts",
    "lint:fix": "eslint --fix lib/**/*.ts",
    "start": "node --unhandled-rejections=strict --trace-warnings --trace-uncaught build/mod.js"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.45",
    "@types/node": "^14.14.21",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-glob": "^3.2.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "type": "module",
  "dependencies": {
    "@benricheson101/i18n": "^1.3.1",
    "@types/ms": "^0.7.31",
    "discord.js": "^12.5.1",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13"
  }
}
